{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Formigo",
  "version": "0.1.0",
  "identifier": "com.bog.formigo",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:9080/app/",
    "frontendDist": "../app/-"
  },
  "app": {
    "windows": [
      {
        "title": "Formigo",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ]
  }
}
